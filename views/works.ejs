<%- include('partials/_header')%>
    <!-- end header section -->
    <!-- slider section -->
  <!-- catagory section -->

  <section class="catagory_section mt-4 mb-4">
    <div class="catagory_container">
      <div class="container ">
        <div class="heading_container heading_center">
          <a class="btn btn-warning mt-2 mb-5" href="/works/add">İş Ekle</a>
          <h2>
            İşler
          </h2>
        </div>
        <div class="row">

          <% for (let i=0; i< works.length; i++) { %>
          <div style="border-bottom: 1px solid black;" class="col-sm-6 col-md-4 ">
            <a href="/dashboard/works/<%=works[i].id%>">
            <div class="box ">
              <div class="img-box">
                <img src="/images/cat2.png" alt="">
              </div>
              <div class="detail-box">
                <h5>
                  <%=works[i].name%>
                </h5>
              </div>
            </div></a>
          </div>
        
          <% } %>

        </div>
        
      </div>
    </div>
  
  </section>

  <%- include('partials/_footer')%>